GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1
HEADER_SEARCH_PATHS = $(inherited) "${PODS_ROOT}/Headers/Public" "${PODS_ROOT}/Headers/Public/Pod1" "${PODS_ROOT}/Headers/Public/Pod2" "${PODS_ROOT}/Headers/Public/Pod3"
LIBRARY_SEARCH_PATHS = $(inherited) "${PODS_CONFIGURATION_BUILD_DIR}/Pod1" "${PODS_CONFIGURATION_BUILD_DIR}/Pod2" "${PODS_CONFIGURATION_BUILD_DIR}/Pod3"
OTHER_LDFLAGS = $(inherited) -ObjC -l"Pod1" -l"Pod2" -l"Pod3"
PODS_BUILD_DIR = ${BUILD_DIR}
PODS_CONFIGURATION_BUILD_DIR = ${PODS_BUILD_DIR}/$(CONFIGURATION)$(EFFECTIVE_PLATFORM_NAME)
PODS_PODFILE_DIR_PATH = ${SRCROOT}/.
PODS_ROOT = ${SRCROOT}/Pods
