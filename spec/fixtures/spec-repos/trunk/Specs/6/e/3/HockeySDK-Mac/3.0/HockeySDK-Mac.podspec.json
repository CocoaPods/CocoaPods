{
  "name": "HockeySDK-Mac",
  "version": "3.0",
  "summary": "Collect crash reports with HockeyApp.",
  "description": "                        HockeyApp is a service to distribute beta apps, collect crash reports and\n                        communicate with your app's users.\n                        \n                        It improves the testing process dramatically and can be used for both beta\n                        and App Store builds.\n",
  "homepage": "http://www.hockeyapp.net/",
  "documentation_url": "http://www.hockeyapp.net/help/sdk/mac/3.0/",
  "license": {
    "type": "MIT",
    "file": "HockeySDK-Mac/LICENSE.txt"
  },
  "authors": {
    "Andreas Linde": "mail@andreaslinde.de",
    "Thomas Dohmke": "thomas@dohmke.de"
  },
  "source": {
    "http": "http://download.hockeyapp.net/sdk/mac/HockeySDK-Mac-3.0.zip"
  },
  "platforms": {
    "osx": 10.7
  },
  "requires_arc": false,
  "vendored_frameworks": "HockeySDK-Mac/HockeySDK.framework",
  "public_header_files": "HockeySDK-Mac/HockeySDK.framework/Headers/*.h*",
  "resources": "HockeySDK-Mac/HockeySDK.framework",
  "xcconfig": {
    "LD_RUNPATH_SEARCH_PATHS": "@loader_path/../Frameworks"
  }
