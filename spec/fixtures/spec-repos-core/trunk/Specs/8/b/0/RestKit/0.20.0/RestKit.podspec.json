    {
      "name": "Core",
      "source_files": [
        "Code/*.h",
        "Vendor/LibComponentLogging/Core",
        "Vendor/LibComponentLogging/NSLog"
      ],
      "header_dir": "RestKit",
      "dependencies": {
        "RestKit/ObjectMapping": [

        ],
        "RestKit/Network": [

        ],
        "RestKit/CoreData": [

        ]
      }
    },
    {
      "name": "ObjectMapping",
      "header_dir": "RestKit/ObjectMapping",
      "source_files": "Code/ObjectMapping"
    },
    {
      "name": "Network",
      "header_dir": "RestKit/Network",
      "source_files": "Code/Network",
      "ios": {
        "frameworks": [
          "CFNetwork",
          "Security",
          "MobileCoreServices",
          "SystemConfiguration"
        ]
      },
      "osx": {
        "frameworks": [
          "CoreServices",
          "Security",
          "SystemConfiguration"
        ]
      },
      "dependencies": {
        "SOCKit": [

        ],
        "AFNetworking": [
          "1.1.0"
        ],
        "RestKit/ObjectMapping": [

        ],
        "RestKit/Support": [

        ]
      }
    },
    {
      "name": "CoreData",
      "header_dir": "RestKit/CoreData",
      "source_files": "Code/CoreData",
      "frameworks": "CoreData"
    },
    {
      "name": "Testing",
      "header_dir": "RestKit/Testing",
      "source_files": "Code/Testing"
    },
    {
      "name": "Search",
      "header_dir": "RestKit/Search",
      "source_files": "Code/Search",
      "dependencies": {
        "RestKit/CoreData": [

        ]
      }
    },
    {
      "name": "Support",
      "header_dir": "RestKit/Support",
      "source_files": "Code/Support"
    }
  ]
}
